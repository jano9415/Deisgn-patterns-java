package adapter;

//Ez az osztály nem implementálja a Vehicle interfészt
public class Bus {
	
	private String name;
	private int maxSpeed;
	private int wheels;
	
	public Bus() {
		
	}
	
	public Bus(String name, int maxSpeed, int wheels) {
		super();
		this.name = name;
		this.maxSpeed = maxSpeed;
		this.wheels = wheels;
	}
	
	

}
