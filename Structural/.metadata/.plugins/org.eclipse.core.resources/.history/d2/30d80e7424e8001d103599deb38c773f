package bridge;

public class Product extends Thing {
	
	
	public Product(View view1, View view2) {
		super(view1, view2);
	}

	@Override
	public void show() {
		System.out.println("Termék megjelenítve:");
		System.out.println("Név: " + this.getName());
		System.out.println("Leírás: " + this.getDescription());
		view1.draw();
		view2.draw();
		
	}
	
	

}
