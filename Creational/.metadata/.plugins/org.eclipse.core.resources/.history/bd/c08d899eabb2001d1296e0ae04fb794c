package abstractfactory;

//Relációs adatbázis gyár osztály
public class RelationalDbFactory extends DbFactory {

	@Override
	public Database getDatabase(String databaseType) {
		switch(databaseType) {
		case "mssql" :
			return new MsSql();
		case "pssql" :
			return new PsSql();
		default :
			throw IllegalArgumentException();
		}
	}

}
