package mediator;

public class User {
	
	private String name;

	public User(String name) {
		super();
		this.name = name;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}
	
	//Üzenet küldése. Átadom az elküldeni kívánt üzenetet, és magamat (a saját objektumomat)
	public void sendMessage(String message) {
		
	}

}
